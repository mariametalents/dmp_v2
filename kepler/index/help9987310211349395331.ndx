action\";\ncreateHtmlContent(\"<h2>Action screen</h2>\");","type":"CODE","items":null}]}{"id":"routines:BA_createInput","url":"https://appfire.atlassian.net/wiki/spaces/PSJ/pages/15488183/BA_createInput","title":"BA_createInput","availability":"1.0","parentId":null,"keywords":["BA_createInput","createInput"],"relatedIds":null,"references":null,"sections":[{"title":"Syntax","body":"use \"poweraction\";\ncreateInput(label, defaultValue, isDisabled[, isRequired, fieldDescription]);\n\n//--------------------or--------------------\n//no package declaration\nBA_createInput(label, defaultValue, isDisabled[, isRequired, fieldDescription]);","type":"CODE","items":null},{"title":"Return Type","body":"none - The returned value has no meaning.","type":"PARA","items":null},{"title":"Description","body":"Creates a simple text input suitable for short values. For longer text values like comments, see BA_createTextArea.","type":"PARA","items":null},{"title":"Example","body":"use \"poweraction\";\ncreateInput(\"Input\", \"My input is ...\", false, false, \"Type your input in here\");","type":"CODE","items":null}]}{"id":"routines:BA_createMultiSelectList","url":"https://appfire.atlassian.net/wiki/spaces/PSJ/pages/15487209/BA_createMultiSelectList","title":"BA_createMultiSelectList","availability":"1.0","parentId":null,"keywords":["BA_createMultiSelectList","createMultiSelectList"],"relatedIds":null,"references":null,"sections":[{"title":"Syntax","body":"use \"poweraction\";\ncreateMultiSelectList(label, options, defaultValues, isDisabled[, isRequired, fieldDescription]);\n\n//--------------------or--------------------\n//no package declaration\nBA_createMultiSelectList(label, options, defaultValues, isDisabled[, isRequired, fieldDescription]);","type":"CODE","items":null},{"title":"Return Type","body":"none - The returned value has no meaning.","type":"PARA","items":null},{"title":"Description","body":"Creates a multi select list.","type":"PARA","items":null},{"title":"Example","body":"use \"poweraction\";\ncreateMultiSelectList(\"Options\", {\"option 1\", \"option 2\", \"option 3\"}, \"\", false, true, \"select some options\");","type":"CODE","items":null}]}{"id":"routines:BA_createRadioGroup","url":"https://appfire.atlassian.net/wiki/spaces/PSJ/pages/15487926/BA_createRadioGroup","title":"BA_createRadioGroup","availability":"1.0","parentId":null,"keywords":["BA_createRadioGroup","createRadioGroup"],"relatedIds":null,"references":null,"sections":[{"title":"Syntax","body":"use \"poweraction\";\ncreateRadioGroup(label, options, defaultValue, isDisabled[, isRequired, fieldDescription]);\n\n//--------------------or--------------------\n//no package declaration\nBA_createRadioGroup(label, options, defaultValue, isDisabled[, isRequired, fieldDescription]);","type":"CODE","items":null},{"title":"Return Type","body":"none - The returned value has no meaning.","type":"PARA","items":null},{"title":"Description","body":"Creates a radio group.","type":"PARA","items":null},{"title":"Example","body":"use \"poweraction\";\ncreateRadioGroup(\"Options\", {\"option 1\", \"option 2\"}, \"\", false, true, \"select some options\");","type":"CODE","items":null}]}{"id":"routines:BA_createSelectList","url":"https://appfire.atlassian.net/wiki/spaces/PSJ/pages/15486981/BA_createSelectList","title":"BA_createSelectList","availability":"1.0","parentId":null,"keywords":["BA_createSelectList","createSelectList"],"relatedIds":null,"references":null,"sections":[{"title":"Syntax","body":"use \"poweraction\";\ncreateSelectList(label, options, defaultValue, isDisabled[, isRequired, fieldDescription]);\n\n//--------------------or--------------------\n//no package declaration\nBA_createSelectList(label, options, defaultValue, isDisabled[, isRequired, fieldDescription]);","type":"CODE","items":null},{"title":"Return Type","body":"none - The returned value has no meaning.","type":"PARA","items":null},{"title":"Description","body":"Creates a single select list.","type":"PARA","items":null},{"title":"Example","body":"use \"poweraction\";\ncreateSelectList(\"Options\", {\"option 1\", \"option 2\"}, \"\", false, true, \"select some options\");","type":"CODE","items":null}]}{"id":"routines:BA_createSingleCheckbox","url":"https://appfire.atlassian.net/wiki/spaces/PSJ/pages/15487074/BA_createSingleCheckbox","title":"BA_createSingleCheckbox","availability":"1.0","parentId":null,"keywords":["BA_createSingleCheckbox","createSingleCheckbox"],"relatedIds":null,"references":null,"sections":[{"title":"Syntax","body":"use \"poweraction\";\ncreateSingleCheckbox(label, isChecked, isDisabled[, isRequired, fieldDescription]);\n\n//--------------------or--------------------\n//no package declaration\nBA_createSingleCheckbox(label, isChecked, isDisabled[, isRequired, fieldDescription])","type":"CODE","items":null},{"title":"Return Type","body":"none - The returned value has no meaning.","type":"PARA","items":null},{"title":"Description","body":"Creates a toggle button.","type":"PARA","items":null},{"title":"Example","body":"use \"poweraction\";\ncreateSingleCheckbox(\"Email confirmation\", false, false, false, \"Check this if you want to receive confirmation\");","type":"CODE","items":null}]}{"id":"routines:BA_createTextArea","url":"https://appfire.atlassian.net/wiki/spaces/PSJ/pages/15488573/BA_createTextArea","title":"BA_createTextArea","availability":"1.0","parentId":null,"keywords":["BA_createTextArea","createTextArea"],"relatedIds":null,"references":null,"sections":[{"title":"Syntax","body":"use \"poweraction\";\ncreateTextArea(label, defaultValue, isDisabled [, rows[, isRequired, fieldDescription]]);\n\n//--------------------or--------------------\n//no package declaration\nBA_createTextArea(label, defaultValue, isDisabled [, rows[, isRequired, fieldDescription]]);","type":"CODE","items":null},{"title":"Return Type","body":"none - The returned value has no meaning.","type":"PARA","items":null},{"title":"Description","body":"Creates a text area suitable for longer text values like comments.","type":"PARA","items":null},{"title":"Example","body":"use \"poweraction\";\ncreateTextArea(\"Description\", \"\", false , 5, false, \"Add your description here.\");","type":"CODE","items":null}]}{"id":"routines:BA_createUserPicker","url":"https://appfire.atlassian.net/wiki/spaces/PSJ/pages/15486808/BA_createUserPicker","title":"BA_createUserPicker","availability":"1.0","parentId":null,"keywords":["BA_createUserPicker","createUserPicker"],"relatedIds":null,"references":null,"sections":[{"title":"Syntax","body":"use \"poweraction\";\ncreateUserPicker(label, defaultValue, isDisabled, isRequired, fieldDescription);\n\n//--------------------or--------------------\n//no package declaration\nBA_createUserPicker(label, defaultValue, isDisabled, isRequired, fieldDescription);","type":"CODE","items":null},{"title":"Return Type","body":"none - The returned value has no meaning.","type":"PARA","items":null},{"title":"Description","body":"Creates a user picker.","type":"PARA","items":null},{"title":"Example","body":"use \"poweraction\";\ncreateUserPicker(\"User\", \"\", false, true, \"Select a user.\");","type":"CODE","items":null}]}{"id":"routines:BA_createWikiContent","url":"https://appfire.atlassian.net/wiki/spaces/PSJ/pages/15487439/BA_createWikiContent","title":"BA_createWikiContent","availability":"1.0","parentId":null,"keywords":["BA_createWikiContent","createWikiContent"],"relatedIds":null,"references":null,"sections":[{"title":"Syntax","body":"use \"poweraction\";\ncreateWikiContent(wikiText);\n\n//--------------------or--------------------\n//no package declaration\nBA_createWikiContent(wikiText);","type":"CODE","items":null},{"title":"Return Type","body":"none - The returned value has no meaning.","type":"PARA","items":null},{"title":"Description","body":"Adds wiki-rendered content to the screen.","type":"PARA","items":null},{"title":"Example","body":"use \"poweraction\";\ncreateWikiContent(\"h1. Wiki Title\");","type":"CODE","items":null}]}{"id":"routines:BA_getBlitzAttribute","url":"https://appfire.atlassian.net/wiki/spaces/PSJ/pages/15487497/BA_getBlitzAttribute","title":"BA_getBlitzAttribute","availability":"1.0","parentId":null,"keywords":["BA_getBlitzAttribute","getBlitzAttribute"],"relatedIds":null,"references":null,"sections":[{"title":"Syntax","body":"use \"poweraction\";\ngetBlitzAttribute(name);\n\n//--------------------or--------------------\n//no package declaration\nBA_getBlitzAttribute(name);","type":"CODE","items":null},{"title":"Return Type","body":"string","type":"PARA","items":null},{"title":"Description","body":"Returns the value stored for the given attribute name or an empty string, if no value is stored for that name. The values are retrieved from the HTTP session, where they have been previously set using the BA_setBlitzAttribute routine.","type":"PARA","items":null},{"title":"Example","body":"use \"poweraction\";\nreturn getBlitzAttribute(\"description\");","type":"CODE","items":null}]}{"id":"routines:BA_setBlitzAttribute","url":"https://appfire.atlassian.net/wiki/spaces/PSJ/pages/15487311/BA_setBlitzAttribute","title":"BA_setBlitzAttribute","availability":"1.0","parentId":null,"keywords":["BA_setBlitzAttribute","setBlitzAttribute"],"relatedIds":null,"references":null,"sections":[{"title":"Syntax","body":"use \"poweraction\";\nsetBlitzAttribute(name, value)\n\n//--------------------or--------------------\n//no package declaration\nBA_setBlitzAttribute(name, value);","type":"CODE","items":null},{"title":"Return Type","body":"none - The returned value has no meaning.","type":"PARA","items":null},{"title":"Description","body":"Stores the given attribute as a (name, value) pair in the HTTP session. The value can be retrieved with the BA_getBlitzAttribute routine.","type":"PARA","items":null},{"title":"Example","body":"use \"poweraction\";\nsetBlitzAttribute(\"description\", \"This is a description.\");","type":"CODE","items":null}]}{"id":"routines:BA_getSingleValue","url":"https://appfire.atlassian.net/wiki/spaces/PSJ/pages/15485150/BA+getSingleValue","title":"BA_getSingleValue","availability":"1.0","parentId":null,"keywords":["BA_getSingleValue","getSingleValue"],"relatedIds":null,"references":null,"sections":[{"title":"Syntax","body":"use \"poweraction\";\ngetSingleValue(argv, label)\n\n//--------------------or--------------------\n//no package declaration\nBA_getSingleValue(argv, label);","type":"CODE","items":null},{"title":"Return Type","body":"string","type":"PARA","items":null},{"title":"Description","body":"Returns the value entered or the selected option for the given label. For a checkbox it will return \"checked\" if the checkbox was selected or an empty string if not.","type":"PARA","items":null},{"title":"Example","body":"use \"poweraction\";\nreturn getSingleValue(argv, \"input\");//where input is set in screen script: createInput(\"input\", \"My input is ...\", false);","type":"CODE","items":null}]}{"id":"routines:BA_getMultiValues","url":"https://appfire.atlassian.net/wiki/spaces/PSJ/pages/15486088/BA_getMultiValues","title":"BA_getMultiValues","availability":"1.0","parentId":null,"keywords":["BA_getMultiValues","getMultiValues"],"relatedIds":null,"references":null,"sections":[{"title":"Syntax","body":"use \"poweraction\";\ngetMultiValues(argv, label)\n\n//--------------------or--------------------\n//no package declaration\nBA_getMultiValues(argv, label);","type":"CODE","items":null},{"title":"Return Type","body":"string[]","type":"PARA","items":null},{"title":"Description","body":"Returns the selected options. For a file upload it will return a string array containing pairs of original filename and uploaded path.","type":"PARA","items":null},{"title":"Example","body":"use \"poweraction\";\nreturn getMultiValues(argv, \"Options\");//where \"Options\" is set in screen script: createMultiSelectList(\"Options\", {\"option 1\", \"option 2\", \"option 3\"}, \"\", false);","type":"CODE","items":null}]}{"id":"routines:BA_isChecked","url":"https://appfire.atlassian.net/wiki/spaces/PSJ/pages/15485335/BA_isChecked","title":"BA_isChecked","availability":"1.0","parentId":null,"keywords":["BA_isChecked","isChecked"],"relatedIds":null,"references":null,"sections":[{"title":"Syntax","body":"use \"poweraction\";\nisChecked(argv, label)\n\n//--------------------or--------------------\n//no package declaration\nBA_isChecked(argv, label);","type":"CODE","items":null},{"title":"Return Type","body":"boolean","type":"PARA","items":null},{"title":"Description","body":"Checks if a toggle button is selected or not.","type":"PARA","items":null},{"title":"Example","body":"use \"poweraction\";\nreturn isChecked(argv, \"Email confirmation\");//where \"Email confirmation\" is set in screen script: createSingleCheckbox(\"Email confirmation\", false, false, false, \"Check this if you want to receive confirmation\");","type":"CODE","items":null}]}{"id":"routines:BA_getDateValue","url":"https://appfire.atlassian.net/wiki/spaces/PSJ/pages/15485385/BA_getDateValue","title":"BA_getDateValue","availability":"1.0","parentId":null,"keywords":["BA_getDateValue","getDateValue"],"relatedIds":null,"references":null,"sections":[{"title":"Syntax","body":"use \"poweraction\";\ngetDateValue(argv, label)\n\n//--------------------or--------------------\n//no package declaration\nBA_getDateValue(argv, label);","type":"CODE","items":null},{"title":"Return Type","body":"date","type":"PARA","items":null},{"title":"Description","body":"Returns a date value.","type":"PARA","items":null},{"title":"Example","body":"use \"poweraction\";\nreturn getDateValue(argv, \"Current date\");//where \"Current date\" is set in screen script: createDatePicker(\"Current date\", currentDate(), true, false, \"Day of completion.\");","type":"CODE","items":null}]}{"id":"structures:BActionError","url":"https://appfire.atlassian.net/wiki/spaces/PSJ/pages/15481131/Action+Script","title":"BActionError","availability":"3.0.0","parentId":null,"keywords":["BActionError"],"relatedIds":["structures:BActionReturn"],"references":null,"sections":[{"title":"Description","body":"Internal representation for a power action error structure.","type":"PARA","items":null},{"title":"Syntax","body":"struct BActionError {\n\tstring field;\n\tstring message;\n}","type":"CODE","items":null}]}{"id":"structures:BActionReturn","url":"https://appfire.atlassian.net/wiki/spaces/PSJ/pages/15481131/Action+Script","title":"BActionReturn","availability":"3.0.0","parentId":null,"keywords":["BActionReturn"],"relatedIds":["structures:BActionError"],"references":null,"sections":[{"title":"Description","body":"Internal representation for a power action return structure.","type":"PARA","items":null},{"title":"Syntax","body":"struct BActionReturn {\n\tboolean disableRefresh;\n\tstring location;\n\tBActionError[] errors;\n}","type":"CODE","items":null}]}{"id":"uihelp:katl:cf","url":null,"title":"Custom Field Info","availability":null,"parentId":null,"keywords":[],"relatedIds":[],"references":[],"sections":[{"title":null,"body":"This tool provides you information about a particular Jira custom field.","type":"PARA","items":null},{"title":null,"body":"You can simply start to search for a custom field by its id or name. After you examine the list of the CFs, click on the 'View' icon","type":"PARA","items":null},{"title":null,"body":"You will be presented information about the CF itself, its configuration (issue types, projects, screens) and SIL usages, if any are detected","type":"PARA","items":null}]}{"id":"uihelp:katl:cfdesc","url":null,"title":"Custom Field Descriptors","availability":null,"parentId":null,"keywords":[],"relatedIds":[],"references":[],"sections":[{"title":null,"body":"This tool lets you specify how we treat a CF in SIL","type":"PARA","items":null},{"title":null,"body":"By default, we try to guess the types of values for the unknown CFs. However, it is always better to have it specified","type":"PARA","items":null},{"title":null,"body":"For simple CFs, unknown to us, it may work if we have the correct implementation. For some overly complicated CFs, or for CFs that return objects available in other plugins, it may not work at all.","type":"PARA","items":null},{"title":"Important","body":"This does not guarantee any results; it may be used as a last resort before asking us for help.","type":"IMPORTANT","items":null},{"title":"It may not work, after all...","body":"If you cannot make it work, ask us. If you want support for a particular CF, let us know. We can add that support programmatically (or tell you how to do it!)","type":"WARN","items":null}]}{"id":"uihelp:katl:cfdesc-type","url":null,"title":"CF Type Key","availability":null,"parentId":null,"keywords":[],"relatedIds":[],"references":[],"sections":[{"title":null,"body":"The CF type and its key","type":"PARA","items":null}]}{"id":"uihelp:katl:cfdesc-fact","url":null,"title":"Factory ID","availability":null,"parentId":null,"keywords":[],"relatedIds":[],"references":[],"sections":[{"title":null,"body":"The SIL factory that creates the value adapter","type":"PARA","items":null}]}{"id":"uihelp:katl:cfdesc-cfs","url":null,"title":"Factory ID","availability":null,"parentId":null,"keywords":[],"relatedIds":[],"references":[],"sections":[{"title":null,"body":"The CFs that are of this type and their IDs. Click on the info icon to see them.","type":"PARA","items":null}]}{"id":"uihelp:katl:cfdesc-act","url":null,"title":"Change factory","availability":null,"parentId":null,"keywords":[],"relatedIds":[],"references":[],"sections":[{"title":null,"body":"Action is only available for unknown fields","type":"PARA","items":null}]}{"id":"sil:jira:contexts","url":"https://appfire.atlassian.net/wiki/spaces/PSJ/pages/15485601/Variable+Resolution","title":"SIL JIRA Contexts","availability":null,"parentId":null,"keywords":["sil","language","sil language","var","vars","variable","variables","context","issue","jira"],"relatedIds":["sil:jira:parent","sil:jira:issuefields"],"references":null,"sections":[{"title":null,"body":"In Jira, SIl has two main contexts: one is at the JIRA level, and one, embedded in the first, is at the issue level.","type":"PARA","items":null},{"title":null,"body":"When the issue context is present, the language exposes several pre-defined variables (the fields of the issue). Without the issue context, those pre-defined variables do not make any sense","type":"PARA","items":null},{"title":"Contexts are important!","body":"// works because we're in a jira context:\nTEST-1.summary = \"Test 1\";\n// works only in an issue context:\nsummary = \"Test 2\";","type":"CODE","items":null},{"title":null,"body":"Of course, issue entry points like postfunctions, conditions and validators on the workflow or listeners on issue-related events, all assume an issue context by default","type":"PARA","items":null},{"title":null,"body":"However, it's not the case for scheduler, or - let's say - user-related listeners; here the only context that's available is the Jira one.","type":"PARA","items":null},{"title":"Find out more (external link)","body":"https://appfire.atlassian.net/wiki/spaces/PSJ/pages/15489652/Simple+Issue+Language+Reference","type":"LINK","items":null}]}{"id":"sil:jira:parent","url":"https://appfire.atlassian.net/wiki/spaces/PSJ/pages/15485601/Variable+Resolution","title":"Referencing parent issue and subtasks","availability":null,"parentId":null,"keywords":["sil","language","sil language","issue","parent","subtask","sub-task","subtasks","sub-tasks"],"relatedIds":["sil:jira:contexts","sil:jira:issuefields"],"references":null,"sections":[{"title":null,"body":"You can always use the following addressing to refer to the parent of one issue:","type":"PARA","items":null},{"title":"Example : parent","body":"//Jira context:\nif(TEST-11.parent != null) {\n    runnerLog(\"TEST-11 has parent:\" + TEST-11.parent);\n}\n//Issue context:\nif(parent != null) {\n    runnerLog( key + \" has parent:\" + parent);\n}","type":"CODE","items":null},{"title":null,"body":"If parent does not exist (or the issue type is not a sub-task) the value of the expression will be null","type":"PARA","items":null},{"title":"Sub-tasks","body":"Subtasks do not have a direct correspondent in the language, unlike the parent. Instead, you are requested to call subtasks():","type":"PARA","items":null},{"title":"Example : sub-tasks","body":"for(string k in subtasks(\"TEST-10\")) {\n    runnerLog(k + \" has parent:\" + %k%.parent);\n}\n//For the current issue:\nfor(string k in subtasks(key)) {\n    runnerLog(k + \" has parent:\" + %k%.parent);\n}","type":"CODE","items":null},{"title":null,"body":"You will notice that we refer the key via a string, or pass it as such.","type":"PARA","items":null}]}{"id":"sil:jira:issuefields","url":"https://appfire.atlassian.net/wiki/spaces/PSJ/pages/15485601/Variable+Resolution","title":"Issue fields","availability":null,"parentId":null,"keywords":["issue","fields","sil language","issue","field","fields","affectedVersions","assignee","attachments","components","created","description","desc","dueDate","due","estimate","est","environment","env","fixVersions","id","issueCreator","issueType","type","issueTypeId","key","labels","originalEstimate","priority","prio","priorityId","project","prj","projectId","reporter","resolution","resolutionDate","resolutionId","securityLevel","security","securityLevelId","securityId","status","statusId","summary","timeSpent","spent","updated","votes","watchers","workflow","wf","workflowId"],"relatedIds":["sil:jira:contexts","sil:jira:parent"],"references":null,"sections":[{"title":null,"body":"Fields are readily available for you to script on them. Note that unlike other variables, you can redefine them, but we do not recommend it since you will hide the standard fields.","type":"PARA","items":null},{"title":"List of issue fields","body":"The following is the list of standard fields (excluding custom fields). Short names are given for the most important ones.","type":"LIST","items":null}]}{"id":"sil:jira:cfs","url":"https://appfire.atlassian.net/wiki/spaces/PSJ/pages/15485601/Variable+Resolution","title":"Referencing custom fields","availability":null,"parentId":null,"keywords":["sil","language","sil language","issue","custom","field","fields","customfield","customfields"],"relatedIds":["sil:jira:persistent","sil:jira:contexts","sil:jira:issuefields","sil:jira:parent"],"references":null,"sections":[{"title":null,"body":"Custom-fields may be accessed as normal, standard, fields: ","type":"LIST","items":null},{"title":"Example","body":"// Assume we have a CF, id (10012), named 'Test Results', alias 'testResults'\n// the following lines are equivalent\ncustomfield_10012 = \"Ok\";\n#{Test Results} = \"Ok\";\ntestResults = \"Ok\";","type":"CODE","items":null},{"title":"Aliases are your friends","body":"Get the habit of using aliases for the CFs you use. You will thank us, sooner or later. We pushed aliases panel in the SIL Manager editor just to help you with that!","type":"IMPORTANT","items":null},{"title":"Don't use the CF name","body":"If you access a CF by name, please understand that if there are 2 different CFs with the same name, the first one will be returned. Avoid it, if possible. Also, for Jira that use multiple languages, the name you see on the screen might not be the name of the field.","type":"WARN","items":null},{"title":null,"body":"Read more:","type":"PARA","items":null},{"title":"Custom Fields Aliases","body":"https://appfire.atlassian.net/wiki/spaces/PSJ/pages/15486340/Custom+Fields+Aliases","type":"LINK","items":null}]}{"id":"sil:jira:persistent","url":"https://appfire.atlassian.net/wiki/spaces/PSJ/pages/15487662/Persistent+Variables","title":"Persistent Variables","availability":null,"parentId":null,"keywords":["sil","language","sil language","var","vars","variable","variables","persistent"],"relatedIds":["routines:setPersistentVar","routines:toJson","routines:fromJson","sil:jira:cfs","sil:jira:parent","sil:jira:issuefields"],"references":null,"sections":[{"title":null,"body":"Persistent variables were introduced in Jira to deal with pieces of information that are linked to the issue, that are used in computation, but are not shown directly on screen.","type":"PARA","items":null},{"title":null,"body":"A persistent variable is introduced by the context the script runs on, and will be initialized just once. If the script runs in the global context, that persistent variable will become global. If the script runs in the issue context, it will be linked to the issue.","type":"PARA","items":null},{"title":"Persistent vars are survivors","body":"The value of a persistent variable survives the script execution. The same script, run twice, will pick up the last value saved by the previous run.","type":"IMPORTANT","items":null},{"title":null,"body":"While the mechanism works only with primitive types (for structs you must manually serialize / deserialize - but take a look on the JSON routines) this gives you unprecedented power; you may use it for:","type":"LIST","items":null},{"title":"Example","body":"persistent number counter = 0; \ncounter ++;\n\nrunnerLog(\"Counter is now :\" + counter);","type":"CODE","items":null},{"title":null,"body":"If you run this multiple times, you will see the counter is incrementing. To programatically manipulate the values, there are routines, see below setPersistentVar() and getPersistentVar() routines","type":"PARA","items":null}]}{"id":"routines:setPersistentVar","url":"https://appfire.atlassian.net/wiki/spaces/PSJ/pages/15489347/setPersistentVar","title":"setPersistentVar","availability":" SIL Engine > 4.1.7","parentId":null,"keywords":["setPersistentVar","persistent"],"relatedIds":["sil:jira:persistent","routines:getPersistentVar"],"references":null,"sections":[{"title":"Syntax","body":"setPersistentVar(issueKey, varName, value) //issue var\n//or\nsetPersistentVar(varName, value) //global","type":"CODE","items":null},{"title":"Description","body":"Sets the value of a persistent var, as a string. 'varName' is the variable introduced as a persistent variable.","type":"PARA","items":null}]}{"id":"routines:getPersistentVar","url":"https://appfire.atlassian.net/wiki/spaces/PSJ/pages/15487928/getPersistentVar","title":"getPersistentVar","availability":" SIL Engine > 4.1.7","parentId":null,"keywords":["getPersistentVar","persistent"],"relatedIds":["sil:jira:persistent","routines:setPersistentVar"],"references":null,"sections":[{"title":"Syntax","body":"getPersistentVar(issueKey, varName) //issue var\n//or\ngetPersistentVar(varName) //global","type":"CODE","items":null},{"title":"Description","body":"Gets the value of a persistent var, as a string. 'varName' is the variable introduced as a persistent variable.","type":"PARA","items":null}]}{"id":"routines:admCreateScreen","url":"https://appfire.atlassian.net/wiki/spaces/PSJ/pages/15488984/admCreateScreen","title":"admCreateScreen","availability":"2.5.19","parentId":null,"keywords":["admCreateScreen","create","screen","adm"],"relatedIds":[],"references":null,"sections":[{"title":"Syntax","body":"//no package needed\nadmCreateScreen(name, description);","type":"CODE","items":null},{"title":"Return type","body":"String","type":"PARA","items":null},{"title":"Description","body":"Creates a new screen.","type":"PARA","items":null},{"title":"Example","body":"admCreateScreen(\"Test Screen\", \"Test screen description\");","type":"CODE","items":null}]}{"id":"routines:admGetProjectIssueSecurityLevelsFor","url":"https://appfire.atlassian.net/wiki/spaces/PSJ/pages/15480178/admGetProjectIssueSecurityLevelsFor","title":"admGetProjectIssueSecurityLevelsFor","availability":"2.5.8","parentId":null,"keywords":["issue","for","project","adm","security","get","admGetProjectIssueSecurityLevelsFor","levels"],"relatedIds":[],"references":null,"sections":[{"title":"Syntax","body":"//no package needed\nadmGetProjectIssueSecurityLevelsFor(pkey, user);","type":"CODE","items":null},{